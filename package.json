{
  "name": "h5-landing-page",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode production",
    "build:page": "vite build --mode production",
    "build:page:test": "vite build --mode test",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.vue",
    "format": "prettier --write .",
    "build:page:meetManage": "cross-env ENTRY_NAME=meetManage vite build"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-pages": "^0.33.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
